<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.ulpgc.maikath101.kevin.homefindergc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".app.AppMediator"
        android:theme="@style/AppTheme">

        <activity android:name=".adminMode.login.LoginActivity"
            android:label="">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".adminMode.selection.SelectionActivity"></activity>
        <activity android:name=".adminMode.removeHouse.RemoveHouseActivity"></activity>
        <activity android:name=".adminMode.addHouse.AddHouseActivity"></activity>

        <activity android:name=".customerMode.startScreen.StartActivity"></activity>
        <activity android:name=".customerMode.homeDetails.HomeDetailsActivity"></activity>
        <activity android:name=".customerMode.aboutUsScreen.AboutUsActivity"></activity>
        <activity android:name=".customerMode.contactScreen.ContactActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>



</manifest>